{
  "name": "@mindfulstudio/tuscany-components",
  "version": "0.2.0",
  "description": "Components for Tuscany",
  "scripts": {
    "develop": "start-storybook -p 5010 --ci",
    "build": "build-storybook --output-dir build",
    "deploy": "npx vercel deploy ./build --no-clipboard --token=$VERCEL_TOKEN --prod",
    "format": "prettier . --write",
    "typecheck": "tsc --noEmit"
  },
  "author": "Jake Elder",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@mindfulstudio/tuscany-tsconfig": "0.2.0",
    "@mindfulstudio/tuscany-types": "0.2.0"
  },
  "exports": {
    "./": "./src"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  }
}
